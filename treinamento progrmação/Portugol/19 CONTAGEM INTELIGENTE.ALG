algoritmo "contagem inteligente"
var
   i, f : inteiro
inicio
      escreva("Inicio: ")
      leia(i)
      escreva("Fim: ")
      leia(f)
      enquanto i < f faca
               escreva(i,"...")
               i <- i + 1
      fimenquanto
      
      enquanto i >= f faca
               escreva(i,"...")
               i <- i - 1
      fimenquanto
fimalgoritmo